@model IEnumerable<AUSMDM.Models.OnlineRateClass>
<h1>Online Rate</h1>
    <div>
        <body>
            <p>

                <form action="/action_page.php">

                    <label for="Start Date">From:</label>
                    <input type="date" id="ToOnlineRateDate" name="Start Date">
                    <label for="End Date">To:</label>
                    <input type="date" id="FromOnlineRateDate" name="End Date">
                    <select id="selector">
                        <!-- Use "select" to create object -->
                        <option value="" disabled selected>Online Rate:</option> <!-- Add all applicable options -->
                        <option>Rate <= 10</option>
                        <option>Rate <= 20</option>
                        <option>Rate <= 30</option>
                        <option>Rate <= 40</option>
                        <option>Rate <= 50</option>
                        <option>Rate <= 60</option>
                        <option>Rate <= 70</option>
                        <option>Rate <= 80</option>
                        <option>Rate <= 90</option>
                        <option>Rate <= 100</option>

                    </select>
                    <form asp-controller="AnalysisController" asp-action="Search" method="get">
                        <!--https://forums.asp.net/t/2093808.aspx?+ASP+NET+5+MVC+6+How+to+make+a+search+box+and+receive+input+value+in+Search+Action+-->
                        <input name="Search" id="Search" placeholder="Name" />
                        <input type="submit" value="Search" />
                        <a asp-action="Search"></a>
                    </form>


                </form>

            </p>

        </body>
    </div>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LatestCom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OnlineTimes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OfflineTimes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OnlineMinutes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OnlineRate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeviceNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeviceType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LatestCom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OnlineTimes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OfflineTimes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OnlineMinutes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OnlineRate)
            </td>
           
        </tr>
}
    </tbody>
</table>
